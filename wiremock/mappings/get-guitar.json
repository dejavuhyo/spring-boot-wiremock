{
  "request": {
    "method": "GET",
    "urlPath": "/api/guitar",
    "queryParameters": {
      "name": {
        "matches": "(Fender|Gibson|PRS)"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "get-guitar-response.json",
    "transformers": ["response-template"]
  }
}